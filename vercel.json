{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "functions": {
    "api/test.js": {
      "maxDuration": 10
    },
    "api/health.js": {
      "maxDuration": 10
    },
    "api/dispatch.js": {
      "maxDuration": 60
    },
    "api/jobs/[id].js": {
      "maxDuration": 10
    }
  }
} 